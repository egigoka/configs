{
    "#tabmovement": true,
    "#tabstospaces": true,
    "autosu": true,
    "clipboard": "terminal",
    "colorscheme": "light",
    "eofnewline": false,
    "hlsearch": true,
    "keepautoindent": true,
    "mkparents": true,
    "paste": true,
    "savecursor": true,
    "saveundo": true,
    "statusformatl": "$(modified)$(filename) $(overwrite)($(line),$(col)) $(status.paste)| ft:$(opt:filetype) | $(opt:fileformat) | $(opt:encoding)"
}
